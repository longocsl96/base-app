{
  "name": "base-app",
  "version": "1.0.0",
  "description": "Base app",
  "author": "longocsl96",
  "license": "MIT",
  "repository": "longocsl96/base-app",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern --css-modules false,cjs",
    "start": "microbundle-crl watch --no-compress --css-modules false --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "axios-cache-adapter": "^2.5.0",
    "axios-extensions": "^3.1.3",
    "babel-plugin-react-intl": "^5.1.18",
    "bootstrap": "^4.4.1",
    "classnames": "~2.2.6",
    "formik": "^2.1.3",
    "gh-pages": "^2.2.0",
    "history": "^4.10.1",
    "intl-messageformat": "^7.8.4",
    "jsonwebtoken": "~8.5.1",
    "moment": "~2.24.0",
    "postcss-rtl": "^1.5.0",
    "prettier": "^2.0.4",
    "prismjs": "^1.19.0",
    "prop-types": "~15.7.2",
    "query-string": "^6.10.1",
    "react": "^16.13.0",
    "react-dom": "^16.12.0",
    "react-feather": "~2.0.3",
    "react-hammerjs": "^1.0.1",
    "react-intl": "^3.11.0",
    "react-perfect-scrollbar": "^1.5.5",
    "react-redux": "~7.1.3",
    "react-ripples": "^2.2.1",
    "react-router-dom": "~5.1.2",
    "react-scripts": "^3.3.0",
    "react-scroll-up": "~1.3.3",
    "react-sidebar": "~3.0.2",
    "react-toastify": "^5.5.0",
    "react-toggle": "~4.1.1",
    "react-topbar-progress-indicator": "^4.1.0",
    "react-treebeard": "~3.2.3",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "redux-debounced": "~0.5.0",
    "redux-persist": "6.0.0",
    "redux-persist-cookie-storage": "^1.0.0",
    "redux-saga": "1.1.3",
    "redux-thunk": "~2.3.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "microbundle-crl": "^0.13.10",
    "node-sass": "^4.14.1",
    "react-app-rewired": "^2.1.5",
    "run-s": "0.0.0"
  },
  "files": [
    "dist"
  ]
}
